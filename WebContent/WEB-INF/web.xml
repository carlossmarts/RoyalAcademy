<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>RoyalAcademy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>Servlet.GetLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/GetLogin</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>listados</servlet-name>
    <servlet-class>Servlet.MgmtListados</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>listados</servlet-name>
    <url-pattern>/getListados</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>ppl</servlet-name>
    <servlet-class>Servlet.MgmtPPL</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ppl</servlet-name>
    <url-pattern>/getRelacionesPPL</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>pais</servlet-name>
    <servlet-class>Servlet.MgmtPais</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pais</servlet-name>
    <url-pattern>/mgmtPais</url-pattern>
  </servlet-mapping>
</web-app>